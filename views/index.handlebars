<header>
    <h1>Eat-Da-Burger!</h1>
    <img src="" alt=""/>
    <p>Submit a burger name below to devour it.</p>
</header>
<main>
    <form>
        <input type="text" placeholder="Yummy Burger" required/>
        <button type="submit">Submit</button>
    </form>
    <div id="burgers">
        <div class="col" id="availableBurgers">
            <ul>
                {{#each burgers}}
                <li>{{burger_name}} <button type="button">Devour It!</button></li>
                {{/each}}
            </ul>
        </div>
        <div class="col" id="devouredBurgers">
            <ul>
                {{#each burgers}}
                <li>{{burger_name}}</li>
                {{/each}}
            </ul>
        </div>
    </div>
</main>